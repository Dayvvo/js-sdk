{
  "name": "alby-js-sdk",
  "version": "1.0.1",
  "description": "Alby OAuth2 Client",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "prebuild": "yarn clean",
    "prepublishOnly": "yarn test",
    "test": "echo tests are missing",
    "clean": "rm -rf dist"
